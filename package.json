{
  "name": "create-keypair",
  "version": "0.1.2",
  "license": "MIT",
  "author": "Joe Flateau",
  "main": "dist/index.js",
  "module": "dist/create-keypair.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests",
    "release": "release-it"
  },
  "bin": {
    "create-keypair": "./dist/create-keypair.cjs.production.min.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/create-keypair.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/create-keypair.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.21",
    "dts-cli": "^1.4.0",
    "husky": "^7.0.4",
    "release-it": "^14.12.5",
    "size-limit": "^7.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "keypair": "^1.0.4",
    "rollup-plugin-preserve-shebang": "^1.0.1"
  },
  "keywords": [
    "cli",
    "pem",
    "keypair"
  ]
}
